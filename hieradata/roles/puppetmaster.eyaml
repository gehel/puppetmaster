puppet_allow:
  - '*.eu-west-1.compute.internal'
  - '*.ledcom.ch'
  - '127.0.0.1'
puppet_db:            'puppetdb'
puppet_dns_alt_names: 'puppet.aws.ledcom.ch, puppet.int.aws.ledcom.ch'
puppet_manifest_path: '$confdir/environments/$environment/site/site.pp'
puppet_mode:          'server'
puppet_module_path:   '/etc/puppet/environments/$environment/modules:/etc/puppet/environments/$environment/dist'
puppet_passenger:     true
puppet_server:        'puppet.int.aws.ledcom.ch'

rundeck_manage_repos: true
rundeck_projects:
  puppetmaster:
    jobs:
      deploy-puppet-modules:
        description:         'Deploy puppet modules via r10k'
        command_exec:        '/usr/local/bin/r10k deploy environment -p'
        command_description: 'r10k deploy'
        format:              'yaml'
        filter:              'ledcom.ch'
